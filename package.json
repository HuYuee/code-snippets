            },
            {
                "language": "javascript",
                "path": "./snippets/inherit.json"
            },
            {
                "language": "javascript",
                "path": "./snippets/js.json"
            },
            {
                "language": "javascript",
                "path": "./snippets/module.json"
            },
            {
                "language": "javascript",
                "path": "./snippets/moment.json"
            },
            {
                "language": "javascript",
                "path": "./snippets/react.json"
            },
            {
                "language": "javascript",
                "path": "./snippets/reg.json"
            },
            {
                "language": "javascript",
                "path": "./snippets/util.json"
            }
        ]
    }
}