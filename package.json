{
    "name": "code-snippets",
    "displayName": "Code-Snippets",
    "description": "This extension contains code snippets for JavaScript.etc",
    "version": "0.0.3",
    "publisher": "erichuyue",
    "icon": "images/javascript.png",
    "scripts": {
        "pub": "vsce publish"
    },
    "engines": {
        "vscode": "^1.24.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/HuYuee/code-snippets"
    },
    "categories": [
        "Snippets"
    ],
    "contributes": {
        "snippets": [{
